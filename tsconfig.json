{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/store/*": [
        "./src/store/*"
      ],
      "@/images/*": [
        "./src/images/*"
      ],
      "@/styles/*": [
        "./src/styles/*"
      ],
      "@/hooks/*": [
        "./src/hooks/*"
      ],
      "@/reducers/*": [
        "./src/reducers/*"
      ],
      "@/utils/*": [
        "./src/utils/*"
      ],
      "@/data/*": [
        "./src/data/*"
      ],
      "@/layouts/*": [
        "./src/layouts/*"
      ],
      "@/actions/*": [
        "./src/actions/*"
      ],
      "@/pages/*": [
        "./src/pages/*"
      ],
      "@/types/*": [
        "./src/types/*"
      ]
    },
    "types": [
      "vite/client",
      "@emotion/react/types/css-prop"
    ]
  },
  "include": [
    "**/*.d.ts",
    "./src/**/*"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}